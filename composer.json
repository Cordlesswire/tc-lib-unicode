{
    "name": "tecnickcom/tc-lib-unicode",
    "description": "PHP library containing Unicode methods",
    "type": "library",
    "homepage": "http://www.tecnick.com",
    "license": "GNU-LGPL v3",
    "keywords": ["tc-lib-unicode", "PDF", "font", "UTF-8"],
    "authors": [
        {
            "name": "Nicola Asuni",
            "email": "info@tecnick.com",
            "role": "lead"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "tecnickcom/tc-lib-unicode-data": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "pdepend/pdepend" : "2.1.*",
        "squizlabs/php_codesniffer": "2.3.*",
        "phpmd/phpmd" : "2.2.*",
        "sebastian/phpcpd" : "2.0.*",
        "phploc/phploc" : "2.1.*",
        "apigen/apigen" : "4.1.*",
        "nikic/php-parser" : "1.3.*",
        "bartlett/php-compatinfo" : "4.3.*"
    },
    "autoload": {
        "psr-4":{
            "Com\\Tecnick\\Unicode\\": "src"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:tecnickcom/tc-lib-unicode-data.git"
        }
    ]
}
